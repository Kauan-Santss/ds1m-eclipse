package br.senai.sp.jandira;

import br.senai.sp.jandira.model.Conta;

public class App {

	public static void main(String[] args) {
		
		Conta contaAnaMaria;
		Conta contaPedro;
		
		contaAnaMaria = new Conta();
		contaPedro = new Conta();
		
		contaAnaMaria.numero = "111-0";
		contaAnaMaria.titular = "Ana Maria Braga";
		contaAnaMaria.tipo = "Corrente";
		contaAnaMaria.saldo = 2000.0;
		contaAnaMaria.ativa = true;
		
		contaPedro.numero = "222-0";
		contaPedro.titular = "Pedro Cabral";
		contaPedro.tipo = "Poupança";
		contaPedro.saldo = 1500.0;
		contaPedro.ativa = true;
		
		System.out.println("Número da Conta: " + contaAnaMaria.numero);
		System.out.println("Nome: " + contaAnaMaria.titular);
		System.out.println("Tipo de Conta: " + contaAnaMaria.tipo);
		System.out.println("Saldo: R$" + contaAnaMaria.saldo);
		System.out.println("\n");
		System.out.println("Número da Conta: " + contaPedro.numero);
		System.out.println("Nome: " + contaPedro.titular);
		System.out.println("Tipo de Conta: " + contaPedro.tipo);
		System.out.println("Saldo: R$" + contaPedro.saldo);
	}

}
